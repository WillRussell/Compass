.grid {
  transition: background 0.5s ease;
  background:rgba(155,179,191, .5);

  &.undocked {
    transition: background 0.5s ease;
    background:rgba(255,255,255,0.5);
  }
}

.top-bar {
  background:transparent;

  transition: padding-top 0.5s ease;
  padding-top:20px;

  transition: height 0.5s ease;
  height:96px;
  .name h1 a {
    transition: color 0.5s ease;
    color:rgba(0, 0, 0, 1);
  }
}
.undocked .top-bar {
  transition: height 0.5s ease;
  height:66px;
  transition: padding-top 0.5s ease;
  padding-top:10px;
  .name h1 a {
    transition: color 0.5s ease;
    color:rgba(black, .8);
  }
}

.top-bar-section li a:not(.button) {
  background:transparent;
}
.undocked .top-bar-section li:not(.has-form) a:not(.button) {
  background:transparent;
}

.top-bar-section ul li {
  background:transparent;
  & > a {
    transition: color 0.5s ease;
    color:rgba(0, 0, 0, 1);
  }
}
.undocked .top-bar-section ul li {
  background:transparent;
  & > a {
    transition: background 0.5s ease;
    color:rgba(252, 252, 252, 1);
  }
}

