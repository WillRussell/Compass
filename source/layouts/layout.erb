<!doctype html>
<html class="no-js" lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= data.page.title || "Compass" %></title>

        <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>

<link rel="stylesheet" type="text/css" href="javascripts/DataTables-1.10.4/media/css/jquery.dataTables.css">


    <%= stylesheet_link_tag "app", "style" %>
    <%= javascript_include_tag "modernizr" %>
    <link href="//maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet">
  </head>

  <body class="<%= page_classes %>">
    <%= yield %>

  <%= javascript_include_tag  "all" %>
  <%= javascript_include_tag  "timeline" %>
  <%= javascript_include_tag  "funding-by-type" %>
  <%= javascript_include_tag  "funding-by-sector" %>
  <%= javascript_include_tag  "influence-chart" %>
  <%= javascript_include_tag  "top-contributors-chart" %>
  <%= javascript_include_tag  "voting-record" %>
  <%= javascript_include_tag  "issue-ratings" %>
  <%= javascript_include_tag  "energy_scatterplot" %>
  <%= javascript_include_tag  "industry_specific_scatterplot" %>


<script src="http://code.highcharts.com/highcharts.js"></script>
<script src="http://code.highcharts.com/highcharts-3d.js"></script>
<script src="http://code.highcharts.com/modules/exporting.js"></script>
<script src="http://code.highcharts.com/modules/data.js"></script>
<script src="http://code.highcharts.com/highcharts-more.js"></script>


<script type="text/javascript" src="http://www.highcharts.com/media/com_demo/highslide-full.min.js"></script>
<script type="text/javascript" src="http://www.highcharts.com/media/com_demo/highslide.config.js" charset="utf-8"></script>


<script type="text/javascript" charset="utf8" src="javascripts/DataTables-1.10.4/media/js/jquery.dataTables.js"></script>

  </body>
</html>
